{
  "name": "tinypost",
  "version": "0.0.1",
  "description": "Backend for tinypost. A social site. ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev:api": "nodemon ./src/index.ts",
    "start:dev:worker": "nodemon ./src/worker.ts",
    "build": "rimraf ./build && tsc",
    "start:prod:api": "npm run build && node build/index.js",
    "start:prod:worker": "npm run build && node build/worker.js",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/germangamboa95/tinypost.git"
  },
  "author": "germangamboa95",
  "license": "MIT ",
  "bugs": {
    "url": "https://github.com/germangamboa95/tinypost/issues"
  },
  "homepage": "https://github.com/germangamboa95/tinypost#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4"
  }
